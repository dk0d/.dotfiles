{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Sonokai Shusia",
  "editor.minimap.enabled": false,
  // "python.jediEnabled": false,
  // "python.linting.pylintEnabled": false,
  "workbench.colorCustomizations": {},
  "emmet.triggerExpansionOnTab": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  // "todo-tree.highlights.defaultHighlight": {
  //   "foreground": "",
  //   "type": "none"
  // },
  "terminal.integrated.inheritEnv": false,
  // "todo-tree.regex.regex": "((//|#|<!--|;|/\\*|^|%)\\s*($TAGS)|^\\s*- \\[ \\])",
  // "todo-tree.tree.showCountsInTree": true,
  "editor.wordWrap": "on",
  "editor.renderLineHighlight": "all",
  "git.ignoreLegacyWarning": true,
  // "todo-tree.tree.showScanModeButton": false,
  "files.watcherExclude": {
    "**/.conda/**": true,
    "**/data/**": true,
    "**/logs/**": true
  },
  "python.linting.pylintEnabled": false,
  "latex-workshop.latex.autoBuild.interval": -1,
  "latex-workshop.latex.build.clearLog.everyRecipeStep.enabled": false,
  "latex-workshop.synctex.afterBuild.enabled": true,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.clean.subfolder.enabled": true,
  "latex-workshop.latex.outDir": "%DIR%/build",
  "latex-workshop.latex.autoClean.run": "onBuilt",
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "latexmk-xelatex",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-xelatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ]
    },
    {
      "name": "lualatexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "latexmk_rconly",
      "command": "latexmk",
      "args": [
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "env": {}
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "%DOCFILE%"
      ],
      "env": {}
    },
    {
      "name": "rnw2tex",
      "command": "Rscript",
      "args": [
        "-e",
        "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "latexmk ðŸ”ƒ",
      "tools": [
        "latexmk"
      ]
    },
    {
      "name": "latexmk (latexmkrc)",
      "tools": [
        "latexmk_rconly"
      ]
    },
    {
      "name": "latexmk-xelatex",
      "tools": [
        "latexmk-xelatex"
      ]
    },
    {
      "name": "latexmk (lualatex)",
      "tools": [
        "lualatexmk"
      ]
    },
    {
      "name": "pdflatex âžž bibtex âžž pdflatexâ€ŠÃ—â€Š2",
      "tools": [
        "pdflatex",
        "bibtex",
        "pdflatex",
        "pdflatex"
      ]
    },
    {
      "name": "Compile Rnw files",
      "tools": [
        "rnw2tex",
        "latexmk"
      ]
    }
  ],
  "python.autoComplete.extraPaths": [
    "/usr/local/anaconda3/",
    "/usr/local/anaconda3/bin/",
    "${workspaceFolder}/__pypackages__/3.9/lib",
    "${workspaceFolder}/__pypackages__/3.10/lib"
  ],
  "python.analysis.extraPaths": [
    "/usr/local/anaconda3/",
    "/usr/local/anaconda3/bin/",
    "${workspaceFolder}/__pypackages__/3.9/lib",
    "${workspaceFolder}/__pypackages__/3.10/lib",
  ],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "java.semanticHighlighting.enabled": true,
  "editor.fontSize": 13,
  "files.autoSave": "onFocusChange",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "cmake.configureOnOpen": true,
  "markdown.preview.fontSize": 12,
  // "workbench.editorAssociations": {
  //   "*.ipynb": "jupyter.notebook.ipynb"
  // },
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "json.maxItemsComputed": 10000,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.quoteProps": "preserve",
  "prettier.singleQuote": true,
  "notebook.lineNumbers": "on",
  "editor.lineNumbers": "relative",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "vim.easymotion": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.leader": "<space>",
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "d",
        "n"
      ],
      "commands": [
        "editor.action.marker.next"
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<C-s>",
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "c"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "<leader>",
        "x"
      ],
      "commands": [
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "f",
        "d"
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "s"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ]
    },
    {
      "before": [
        "<leader>",
        "s",
        "k"
      ],
      "commands": [
        "workbench.action.openGlobalKeybindings"
      ]
    },
    {
      "before": [
        "<leader>",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.focus"
      ]
    },
    {
      "before": [
        "<leader>",
        "<leader>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<leader>",
        "r"
      ],
      "commands": [
        "editor.action.rename"
      ]
    },
    {
      "before": [
        "<leader>",
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "l"
      ],
      "commands": [
        "workbench.action.focusNextGroup"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        "g",
        "e"
      ],
      "commands": [
        "editor.action.marker.next"
      ]
    },
    {
      "before": [
        "g",
        "E"
      ],
      "commands": [
        "editor.action.marker.prev"
      ]
    },
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "r"
      ],
      "commands": [
        "workbench.action.debug.run"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "s"
      ],
      "commands": [
        "workbench.action.debug.start"
      ],
      "when": "debuggersAvailable && !inDebugMode"
    },
    {
      "before": [
        "<leader>",
        "r",
        "s"
      ],
      "commands": [
        "workbench.action.debug.restart"
      ],
      "when": "inDebugMode"
    },
    {
      "before": [
        "<leader>",
        "r",
        "c"
      ],
      "commands": [
        "workbench.action.debug.continue"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "x"
      ],
      "commands": [
        "workbench.action.debug.stop"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "o"
      ],
      "commands": [
        "workbench.action.debug.stepOver"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "i"
      ],
      "commands": [
        "workbench.action.debug.stepInto"
      ]
    },
    {
      "before": [
        "<leader>",
        "r",
        "u"
      ],
      "commands": [
        "workbench.action.debug.stepOut"
      ]
    },
    {
      "before": [
        "<leader>",
        "x",
        "?"
      ],
      "commands": [
        "workbench.action.debug.selectandstart"
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "editor.debug.action.toggleBreakpoint"
      ]
    },
    {
      "before": [
        "<C-o>"
      ],
      "commands": [
        "workbench.action.navigateBack"
      ]
    },
    {
      "before": [
        "<C-i>"
      ],
      "commands": [
        "workbench.action.navigateForward"
      ]
    },
    {
      "before": [
        "<C-l>"
      ],
      "commands": [
        "workbench.action.focusNextGroup"
      ]
    },
    {
      "before": [
        "<C-h>"
      ],
      "commands": [
        "workbench.action.focusPreviousGroup"
      ]
    },
    {
      "before": [
        "<leader>",
        "o",
        "i"
      ],
      "commands": [
        "editor.action.organizeImports"
      ]
    },
    {
      "before": [
        "<leader>",
        "k"
      ],
      "commands": [
        "git.commitAll"
      ]
    },
    {
      "before": [
        "<leader>",
        "g",
        "p"
      ],
      "commands": [
        "git.push"
      ]
    }
  ],
  "vim.autoindent": true,
  // "vim.neovimPath": "", // if left empty looks in the PATH for nvim
  "vim.enableNeovim": true,
  "git.autofetch": true,
  "vim.showMarksInGutter": true,
  "git.repositoryScanIgnoredFolders": [
    "node_modules",
    "__pycache__"
  ],
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "jupyter.runStartupCommands": [
    "%load_ext autoreload",
    "%autoreload 2"
  ],
  "jupyter.askForKernelRestart": false,
  "[python]": {
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true
  },
  "cSpell.enabled": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.tabWidth": 4,
  "editor.fontLigatures": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.fontFamily": "'FiraCode Nerd Font Mono', Monaco, 'Courier New', monospace",
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.currentLine.enabled": false,
  "gitlens.blame.highlight.locations": [
    "gutter",
    "overview"
  ],
  "gitlens.blame.separateLines": false,
  "latex-workshop.latex.autoBuild.run": "never",
  "git.ignoreRebaseWarning": true,
  "terminal.external.osxExec": "iTerm.app",
  "material-icon-theme.hidesExplorerArrows": false,
  "materialTheme.accent": "Yellow",
  "debug.allowBreakpointsEverywhere": true,
  "debug.console.fontSize": 14,
  "debug.console.historySuggestions": false,
  "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "vetur.format.options.tabSize": 4,
  "xml.format.splitAttributesIndentSize": 4,
  "redhat.telemetry.enabled": false,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.gpuAcceleration": "on",
  "remote.SSH.showLoginTerminal": true,
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "latex-workshop.latex.texDirs": [
    "${workspaceFolder}/src"
  ],
  "latex-workshop.intellisense.package.dirs": [
    "${workspaceFolder}/packages"
  ],
  "git.openRepositoryInParentFolders": "never"
}